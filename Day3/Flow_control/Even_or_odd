// to check if a given number is odd or even. 

import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
         int n=s.nextInt();
         if(n%2==0)
         {
            System.out.println("Even number"); 
         }
         else
         {
             System.out.println("Odd number");
         }
	}
}
